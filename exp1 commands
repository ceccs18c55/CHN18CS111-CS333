CREATE TABLE STUDENT(SNO INTEGER NOT NULL,NAME CHAR(20) NOT NULL,MARKS FLOAT NOT NULL,DEPT CHAR(20) NOT NULL);
ALTER TABLE STUDENT ADD AGE INTEGER NOT NULL;
AlTER TABLE STUDENT MODIFY COLUMN DEPT CHAR(15);
ALTER TABLE STUDENT DROP COLUMN MARKS;
ALTER TABLE STUDENT RENAME TO STUDENTS;
INSERT INTO STUDENTS VALUES(55,'SHILPA','CSE',20);
